{
  "name": "basilisk-mining-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@galacticcouncil/math": "https://github.com/galacticcouncil/HydraDX-wasm",
    "@polkadot/api": "^9.3.3",
    "@polkadot/types": "^9.3.3",
    "@polkadot/typegen": "^9.3.3",
    "@polkadot/extension-dapp": "^0.44.5",
    "@polkadot/extension-inject": "^0.44.5",
    "@polkadot/react-identicon": "^2.9.5",
    "@polkadot/util": "^10.1.7",
    "@polkadot/keyring": "^10.1.7",
    "@polkadot/util-crypto": "^10.1.7",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-label": "^1.0.0",
    "@radix-ui/react-portal": "^1.0.0",
    "@radix-ui/react-separator": "^1.0.0",
    "@radix-ui/react-slider": "^1.0.0",
    "@radix-ui/react-switch": "^1.0.0",
    "@tanstack/react-query": "^4.1.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "bignumber.js": "^9.1.0",
    "date-fns": "^2.29.1",
    "i18next": "^21.8.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.3",
    "react-is": "^18.2.0",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^6.3.0",
    "recharts": "^2.1.13",
    "styled-components": "^5.3.5",
    "styled-normalize": "^8.0.7",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4",
    "zustand": "^4.1.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006 ./src",
    "build-storybook": "build-storybook ./src",
    "lint": "run-p -lsc 'lint:*'",
    "lint:ts": "tsc --noEmit -p tsconfig.json",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier -l \"src/**/*.{ts,tsx,json}\"",
    "fix": "run-p -lsc 'fix:*'",
    "fix:eslint": "eslint --fix src",
    "fix:prettier": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "generate": "yarn generate:defs && yarn generate:meta && yarn fix:prettier",
    "generate:defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --package interfaces --input ./src/interfaces --endpoint wss://rpc01.hydration.dev",
    "generate:meta": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --package interfaces --endpoint wss://rpc01.hydration.dev --output ./src/interfaces"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-typescript": "^7.18.12",
    "@babel/preset-typescript": "^7.18.6",
    "@open-web3/orml-types": "^2.0.1",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.47",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "@vitejs/plugin-react": "^2.0.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.22.0",
    "eslint-config-react-app": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.8.1",
    "ts-node": "^10.9.1",
    "vite": "^3.0.7",
    "vite-plugin-svgr": "^2.2.1",
    "vite-tsconfig-paths": "^3.5.0"
  }
}
